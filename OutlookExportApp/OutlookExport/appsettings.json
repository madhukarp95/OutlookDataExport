{
  "ConfigOptions": {
    "InboxColumns": [
      "Subject",
      "Body",
      "From (Name)",
      "From (Address)",
      "From (Type)",
      "Receiver",
      "CC",
      "Bcc",
      "Category",
      "Sensitivity",
      "Importance"
    ],
    "CalendarColumns": [
      "Subject",
      "Body",
      "All day event",
      "Duration",
      "Is Recurring",
      "Location",
      "Organizer",
      "Required Attendees",
      "Optional Attendees",
      "Recepients"
    ],
    "SentItemColumns": [
      "Subject",
      "Body",
      "From (Name)",
      "From (Address)",
      //"From (Type)",
      //"To (Name)",
      //To (Address)
      //"CC",
      //"Bcc",
      //"Category",
      //"Sensitivity",
      "Importance"
    ]
  },

  "Serilog": {
    "Using": [
      "Serilog.Sinks.Async",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console"
    ],

    "MinimumLevel": {
      "Default": "Information", //  Information  - Possible values: Verbose, Debug, Information, Warning, Error, Fatal
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },

    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}]{NewLine}{Message:lj}{NewLine}{Properties}{Exception}{NewLine}"
        }
      },
      {
        "Name": "Async",
        "Args": {
          "bufferSize": 10000,
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "C:/Temp/OutlookDataExport-.log",
                "rollingInterval": "Day",
                "fileSizeLimitBytes": 10485760,
                "rollOnFileSizeLimit": true,
                "retainedFileCountLimit": 90,
                "shared": true,
                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}]{NewLine}{Message:lj}{NewLine}{Properties:lj}{NewLine}{Exception}{NewLine}"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "OutlookDataExport"
    }
  }
}
